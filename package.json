{
  "name": "ssec-web",
  "description": "Website for Silampari Sriwijaya Eye Centre Lubuklinggau",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/styles/app.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/styles/app.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "dev": "concurrently --kill-others -n Tailwind,Sapper \"npm run watch:tailwind\" \"sapper dev\"",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "concurrently \"npm:dev\" \"cy:run\""
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "concurrently": "^5.2.0",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-image": "^0.2.3",
    "tailwindcss": "^1.4.6"
  }
}
